  "dependencies": {
    "chalk": "^4.1.0",
    "cli-select": "^1.1.2",
    "commander": "^8.1.0",
    "cpy": "^8.1.2",
    "cross-spawn": "^7.0.3",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.14.37",
    "@types/rimraf": "^3.0.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.2.3"
  }
